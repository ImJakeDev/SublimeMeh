{
  "name": "sublimemeh",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,json}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "typescript": "latest"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
